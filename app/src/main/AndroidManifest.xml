<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zs26app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_zs"
        android:label="张升"
        android:roundIcon="@mipmap/ic_zs2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:testOnly="false">
        <activity android:name="com.zhangsheng26.Lect10Net.SignActivity"></activity>
        <activity android:name="com.zhangsheng26.Lect10Net.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zhangsheng26.Lect10Net.list.NetListActivity"></activity>
        <activity
            android:name="com.zhangsheng26.Lect10Net.WebActivity"
            android:theme="@style/ActNoActionBar"></activity>
        <activity android:name="com.zhangsheng26.Lect10Net.NetActivity"></activity>

    </application>

</manifest>